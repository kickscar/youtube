{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "ie": "11",
          "edge": "80",
          "firefox": "73",
          "chrome": "82",
          "opera": "69",
          "safari": "13"
        }
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "compilerOptions": {
          "target": "es6",
          "moduleResolution": "node",
          "allowJs": true,
          "checkJs": true,
          "noEmit": true,
          "strict": true,
          "isolatedModules": true,
          "esModuleInterop": true,
          "noImplicitAny": false,
          "sourceMap": false
        },
        "include": [
          "src"
        ],
        "exclude": [
          "node_modules"
        ]
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread"
  ]
}